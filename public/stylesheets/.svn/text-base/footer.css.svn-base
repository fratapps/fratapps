#footer {
	background: #FFFFFF;
    clear: both;
    text-align: center;
	font-size: 10pt;
	color: #3399CC;
    margin-left: auto;
    margin-right: auto;
    padding: 10px;
    margin-top: 50px;
    width: 960px;
}

#footer li {
    font-weight: bold;
	color: #3399CC;
    display: inline;
    margin: 1px;
    padding: 0.1em 0.5em 0.2em;
}

#footer ul {
    margin: 0;
    padding: 0;
   color: #3399CC;
}
