<h1>Listing poll_votes</h1>

<table>
  <tr>
    <th>Poll question</th>
    <th>Poll answer</th>
    <th>User</th>
  </tr>

<% @poll_votes.each do |poll_vote| %>
  <tr>
    <td><%=h poll_vote.poll_question_id %></td>
    <td><%=h poll_vote.poll_answer_id %></td>
    <td><%=h poll_vote.user_id %></td>
    <td><%= link_to 'Show', poll_vote %></td>
    <td><%= link_to 'Edit', edit_poll_vote_path(poll_vote) %></td>
    <td><%= link_to 'Destroy', poll_vote, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New poll_vote', new_poll_vote_path %>